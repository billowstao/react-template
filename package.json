{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18",
    "npm": ">=9",
    "pnpm": ">=7"
  },
  "packageManager": "pnpm@7.25.0",
  "scripts": {
    "prepare": "pnpm exec ts-node --project ./tsconfig.node.json ./config/scripts/prepare.ts",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.27",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react-swc": "^3.0.1",
    "husky": "^8.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
  }
}

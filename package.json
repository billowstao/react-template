{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18",
    "npm": ">=9",
    "pnpm": ">=7"
  },
  "packageManager": "pnpm@8.12.0+sha256.553e4eb0e2a2c9abcb419b3262bdc7aee8ae3c42e2301a1807d44575786160c9",
  "scripts": {
    "prepare": "node ./config/scripts/prepare.mjs",
    "commit": "cz",
    "lint": "pnpm run lint:es && pnpm run lint:style",
    "lint:es": "pnpm exec eslint --fix ./",
    "lint:style": "pnpm exec stylelint \"**/*.{css,html}\" --fix",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
